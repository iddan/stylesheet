{
  "name": "stylesheet",
  "version": "0.6.0",
  "description": "Dynamic CSS for user interfaces",
  "scripts": {
    "build": "yarn run build:dom; yarn run build:vanilla-dom; yarn run build:react-dom; yarn run build:examples-webpack-react-dom",
    "build:dom": "babel dom/src --out-dir dom/dist",
    "build:vanilla-dom": "babel vanilla-dom/src --out-dir vanilla-dom/dist",
    "build:react-dom": "babel react-dom/src --out-dir react-dom/dist",
    "build:examples-webpack-react-dom": "cd examples/webpack-react-dom; yarn run build -- --verbose --bail"
  },
  "keywords": [
    "css-modules",
    "webpack",
    "react",
    "css",
    "dynamic-style-sheets"
  ],
  "bugs": {
    "url": "https://github.com/500tech/stylesheet/issues",
    "email": "iddan@500tech.com"
  },
  "license": "MIT",
  "files": [
    "core/**",
    "loader/**",
    "react-dom/**"
  ],
  "main": " ",
  "repository": {
    "type": "git",
    "url": "https://github.com/500tech/stylesheet.git"
  },
  "directories": {},
  "author": {
    "name": "500Tech",
    "email": "info@500tech.com",
    "url": "500tech.com"
  },
  "contributors": [{
    "name": "Iddan Aharonson",
    "email": "mail@aniddan.com",
    "url": "aniddan.com"
  }],
  "dependencies": {
    "babel-core": "^6.24.1",
    "css-loader": "^0.28.3",
    "loader-utils": "^1.0.4",
    "lodash": "^4.17.4",
    "postcss": "^5.2.16",
    "postcss-selector-parser": "^2.2.3",
    "postcss-value-parser": "^3.3.0",
    "react": "^15.4.2",
    "shortid": "^2.2.8",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.2.2",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-fbjs-opensource": "^1.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ]
  }
